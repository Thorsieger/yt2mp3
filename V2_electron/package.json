{
  "name": "yt2mp3",
  "version": "1.0.0",
  "description": "Download youtube playlist to mp3",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  ",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 ",    
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64"
  },
  "author": "Thorsieger",
  "license": "ISC",
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^3.1.2",
    "ytpl": "^0.2.0"
  },
  "devDependencies": {
    "electron": "^9.1.1",
    "electron-packager": "^15.0.0",
    "electron-winstaller": "^4.0.1"
  }
}
