{
  "name": "yt2mp3",
  "version": "1.0.0",
  "description": "Download youtube playlist to mp3",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-builder build --mac",
    "package-linux": "electron-builder build --linux",
    "package-win": "electron-builder build --win --x64"
  },
  "build": {
    "appId": "com.electron.udownload",
    "asar" : "false",
    "files": [
      "!ffmpeg/*",
      "!dist/"
    ],
    "linux": {
      "target": "AppImage"
    }
  },
  "author": "Thorsieger",
  "license": "MIT",
  "dependencies": {
    "ffmpeg-static": "^4.2.6",
    "fluent-ffmpeg": "^2.1.2",
    "ytdl-core": "^3.1.2",
    "ytpl": "^0.2.0"
  },
  "devDependencies": {
    "electron": "^9.1.1",
    "electron-builder": "^22.7.0",
    "yarn": "^1.22.4"
  }
}
